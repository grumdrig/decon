import "standard.con"

Word: signed size 16 byte

Big: bigendian { Word word }
Little: littleendian { Word word }

terminator = 'T' char
TString: char[through terminator]

Test: {
  char[5] "#abcd" f1
  char[5]
  char[10] f2
  {
    bool bool
    null null
  }[2] nada
   
  char[before 'L']
  "LMNOP"
  char Q
  TString "RST" totee

  byte[before '1']
  Little little
  Big big

  # char["drop"]  

  char[through "["]
  char[before ']']  one
  char[2] apple
  byte[through 'o'] two
  char closebracket
  "[three]"
  "OK" ok

  # Ignore the tail
  char[]
}
