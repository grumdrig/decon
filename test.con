#abcdefghijklmnopqrstuvwxyz
#ABCDEFGHIJKLMNOPQRSTUVWXYZ
#19191919191919191919191919
#[one][two][three]OK

import "standard.con"

Word: signed size 16 byte

Big: bigendian { word Word }
Little: littleendian { word Word }

terminator = 'T' char
TString: char[through terminator]

Test: {
  f1 "#abcd" char[5]
  _ char[5]
  f2 char[10]
  nada {
    b bool
    n null
  }[2]
   
  _ char[before 'L']
  "LMNOP"
  q char
  totee "RST" TString

  _ byte[before '1']
  little Little
  big Big

  # egg char["drop"]  

  _ char[through "["]
  one char[before ']']
  apple char[2]
  two byte[through 'o']
  f5 char
  "[three]"
  ok "OK"

  # Ignore the tail
  _ char[]
}
